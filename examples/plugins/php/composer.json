{
  "name": "dragonfly/php-plugin-example",
  "description": "Example PHP plugin for Dragonfly using gRPC",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "grpc/grpc": "1.74.0",
    "google/protobuf": "^3.25"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10"
  },
  "autoload": {
    "psr-4": {
      "Df\\": "../../../proto/generated/php/Df/",
      "Dragonfly\\PluginLib\\": "lib/",
      "ExamplePhp\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
